{
  "AWS": {
    "Region": "eu-central-1",
    "CloudWatch": {
      "LogGroupName": "<yourAppName>-auth"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "Fatal"
      }
    },
    "Properties": {
      "Application": "<yourAppName>.Auth"
    }
  }
}